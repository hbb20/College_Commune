        <servlet-name>s2_f_signup</servlet-name>
        <url-pattern>/s2_f_signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s2_f_notice_add</servlet-name>
        <url-pattern>/s2_f_notice_add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s2_da_req_delete</servlet-name>
        <url-pattern>/s2_da_req_delete</url-pattern>
