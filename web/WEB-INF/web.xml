    <servlet-mapping>
        <servlet-name>s2_da_notice_delete</servlet-name>
        <url-pattern>/s2_da_notice_delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s2_da_canvas_add</servlet-name>
        <url-pattern>/s2_da_canvas_add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>jM1_startup.jsp</welcome-file>
    </welcome-file-list>
</web-app>
